<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="fill_parent"
    android:layout_height="fill_parent">
    <Button android:id="@+id/rgbButton" android:layout_width="fill_parent"
        android:layout_height="wrap_content" android:text="RGB" />
    <Button android:id="@+id/hsvButton" android:layout_width="fill_parent"
        android:layout_height="wrap_content" android:text="HSV" />
    <Button android:id="@+id/argbButton" android:layout_width="fill_parent"
        android:layout_height="wrap_content" android:text="ARGB" />
    <Button android:id="@+id/cmykButton" android:layout_width="fill_parent"
        android:layout_height="wrap_content" android:text="CMYK" />
</LinearLayout>
