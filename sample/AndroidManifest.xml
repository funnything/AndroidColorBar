<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="jp.funnything.colorbar" android:versionCode="1"
    android:versionName="1.0">
    <application android:icon="@drawable/icon" android:label="@string/app_name">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RGBActivity" android:theme="@android:style/Theme.Dialog" />
        <activity android:name=".HSVActivity" android:theme="@android:style/Theme.Dialog" />
        <activity android:name=".ARGBActivity" android:theme="@android:style/Theme.Dialog" />
        <activity android:name=".CMYKActivity" android:theme="@android:style/Theme.Dialog" />
    </application>
    <uses-sdk android:minSdkVersion="4" />
</manifest> 